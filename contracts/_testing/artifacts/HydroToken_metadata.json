					"params": {
						"_amount": "The amount of tokens to be approved for transfer",
						"_spender": "The address of the account able to transfer the tokens"
					},
					"return": "True if the approval was successful"
				},
				"balanceOf(address)": {
					"return": "The balance of `_owner`"
				},
